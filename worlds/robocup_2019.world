<?xml version='1.0'?>
<sdf version='1.6'>
  <world name='default'>

    <!-- Camera Pose -->
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>-11 -0 11 0 0.7853 0</pose>
      </camera>
    </gui>

    <!-- physics -->
    <physics type='ode'>
      <max_step_size>0.001</max_step_size>
      <real_time_update_rate>1000</real_time_update_rate>
    </physics>

    <!-- sun -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Ground Plane -->
    <include>
      <uri>model://my_ground_plane</uri>
    </include>

    <!-- @HOME 2019 - forniture -->
    <include>
      <name>robocup_2019</name>
      <uri>model://robocup_2019</uri>
      <pose>0 0 0.1 0 0 0</pose>
    </include>

  </world>
</sdf>
